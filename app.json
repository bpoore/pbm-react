{
  "expo": {
    "orientation": "portrait",
    "assetBundlePatterns": [
      "app/assets/images/*"
    ],
    "androidStatusBar": {
      "barStyle": "dark-content",
      "backgroundColor": "#f5fbff"
    },
    "splash": {
      "image": "app/assets/images/t-shirt-logo@3x.png",
      "backgroundColor": "#4b5862",
      "resizeMode": "contain"
    },
    "updates": {
      "enabled": false
    },
    "version": "5.1.8",
    "slug": "pbm-app",
    "name": "Pinball Map",
    "scheme": "pinballmap",
    "description": "Find public places to play pinball! Pinball Map is kept up to date by users and lists over 20,000 pinball machines.",
    "privacy": "public",
    "githubUrl": "https://github.com/bpoore/pbm-react/",
    "primaryColor": "#d3ecff",
    "ios": {
      "bundleIdentifier": "net.isaacruiz.ppm",
      "userInterfaceStyle": "automatic",
      "icon": "app/assets/images/ios-icon.png",
      "buildNumber": "80",
      "supportsTablet": true,
      "infoPlist": {
        "NSLocationWhenInUseUsageDescription": "Allow access to device location to show nearby places with pinball machines"
      },
      "associatedDomains": ["applinks:pinballmap.com","applinks:www.pinballmap.com"],
      "appStoreUrl": "https://itunes.apple.com/us/app/pinball-map/id359275713?mt=8",
      "config": {
        "googleMapsApiKey": "key",
        "usesNonExemptEncryption": false
      },
      "splash": {
        "image": "app/assets/images/t-shirt-logo@3x.png",
        "backgroundColor": "#4b5862",
        "resizeMode": "contain"
      }
    },
    "android": {
      "package": "com.pbm",
      "userInterfaceStyle": "automatic",
      "config": {
        "googleMaps": {
          "apiKey": "key"
        },
        "googleSignIn": {
          "apiKey": "key"
        }
      },
      "permissions": [
        "ACCESS_COARSE_LOCATION",
        "ACCESS_FINE_LOCATION",
        "WRITE_EXTERNAL_STORAGE",
        "com.google.android.providers.gsf.permission.READ_GSERVICES"
      ],
      "versionCode": 58,
      "adaptiveIcon": {
        "backgroundColor": "#f2e6f1",
        "foregroundImage": "app/assets/images/adaptive-foreground.png"
      },
      "splash": {
        "mdpi": "app/assets/images/res/drawable-mdpi/t-shirt-logo.png",
        "hdpi": "app/assets/images/res/drawable-hdpi/t-shirt-logo.png",
        "xhdpi": "app/assets/images/res/drawable-xhdpi/t-shirt-logo.png",
        "xxhdpi": "app/assets/images/res/drawable-xxhdpi/t-shirt-logo.png",
        "xxxhdpi": "app/assets/images/res/drawable-xxxhdpi/t-shirt-logo.png",
        "backgroundColor": "#4b5862",
        "resizeMode": "contain"
      },
      "intentFilters": [
        {
          "action": "VIEW",
          "data": [
            {
              "scheme": "https",
              "host": "pinballmap.com",
              "pathPrefix": "/map"
            },
          ],
          "category": [
            "BROWSABLE",
            "DEFAULT"
          ]
        }
      ],
      "playStoreUrl": "https://play.google.com/store/apps/details?id=com.pbm&hl=en",
      "icon": "app/assets/images/android-icon.png"
    },
    "platforms": [
      "android",
      "ios"
    ]
  }
}
